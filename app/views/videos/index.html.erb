<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Kind</th>
    </tr>
  </thead>
  <tbody>
    <% @videos.each do |video| %>
    <tr>
      <td>
        <%= link_to video.title, video_path(video) %>
      </td>
      <td>
        <%= video.kind %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
